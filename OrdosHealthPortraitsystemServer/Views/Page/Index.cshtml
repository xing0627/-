    <script type="text/javascript">
        $(function () {
            $(".search").click(function () {
                $.ajax({
                    url: '@Url.Content("~/HttpServer/HealtyInfo")',
                    type: 'post',
                    data: { idCard: $("#text").val(), passWord: $("#text").val().substring($("#text").val().length - 6) },
                    success: function (msg) {
                        if (msg.Code == 2) {
                            alert(msg.Msg)
                        } else {
                            let res = JSON.parse(msg.dataContent);
                            let SelfInfo = JSON.parse(res.MySelfInfo).ordosBasicinfo;
                            let HealthCondition = JSON.parse(res.getHealthCondition).healthCondition;
                            let data = JSON.parse(res.HealthDatumInfo).jkda;
                                //设置姓名
                                $(".name").html(SelfInfo.name)
